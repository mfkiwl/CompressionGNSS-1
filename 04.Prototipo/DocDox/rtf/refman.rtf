\par
{
{\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
Receive GPs data and process it. }}\par
{
\pard\plain \s41\li360\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid 19 \{\par
20     {\cf20 // GPS data collection}\par
21     {\cf19 while} (SERIALGPS.available() > 0)\{\par
22         gps.encode(SERIALGPS.read());\par
23     \}\par
24 \par
25     {\cf20 // Updates STATUS WORD}\par
26     {\cf19 if} (gps.date.isValid())                             {\cf20 //Date}\par
27     \{\par
28         resetSTGPSDate;\par
29         {\cf19 if} (gps.date.isUpdated())\par
30         \{\par
31             LCD_UpdateDate(gps.date.year(), gps.date.month(), gps.date.day());\par
32         \}\par
33     \}\par
34     {\cf19 else}\par
35     \{\par
36         setSTGPSDate;\par
37         LCD_UpdateDate(0,0,0);\par
38     \}\par
39 \par
40     {\cf19 if} (gps.time.isValid())                             {\cf20 //Time}\par
41     \{\par
42         resetSTGPSTime;\par
43         {\cf19 if} (gps.time.isUpdated())\par
44         \{\par
45             LCD_UpdateTime(gps.time.hour(), gps.time.minute(), gps.time.second());\par
46         \}\par
47     \}\par
48     {\cf19 else}\par
49     \{\par
50         setSTGPSTime;\par
51         LCD_UpdateTime(0,0,0);\par
52     \}\par
53 \par
54 \par
55     {\cf19 if} (gps.satellites.isValid())\par
56     \{\par
57         {\cf19 if} (gps.satellites.isUpdated())\par
58         \{\par
59             LCD_UpdateSat(gps.satellites.value());\par
60         \}\par
61     \}{\cf19 else}\par
62     \{\par
63         LCD_UpdateSat(0);\par
64     \}   \par
65     \par
66 \}\par
}
}
